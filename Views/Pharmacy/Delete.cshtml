@model Pharmacy


<h1>Are you sure you want to delete this pharmacy?</h1>
<form method="post" asp-action="Delete" asp-route-id="@Model.PharmacyId" enctype="multipart/form-data">
    <input type="hidden" value="CategoryId" id="Categoryid" />
    <div class="form-group">
        <input asp-for="Address" class="form-control" placeholder="Address" readonly>
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>

</form>
<div class="form-group">
    <form asp-action="Delete" method="post" asp-route-id="@Model.PharmacyId">
        <input type="hidden" asp-for="PharmacyId" />
        <input type="submit" value="Confirm" class="btn btn-danger float-right mt-3" />
    </form>
</div>